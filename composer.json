{
    "name": "ahmedash95/laravel-facebook-poster-notification",
    "description": "Laravel Notification for posting on Facebook",
    "authors": [{
        "name": "Ahmed Ashraf",
        "email": "ahmed29329@gmail.com"
    }],
    "require": {
        "facebook/graph-sdk": "^5.4",
        "illuminate/notifications": "5.3.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "3.3.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\FacebookPoster\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\FacebookPoster\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}